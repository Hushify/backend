{
    "Serilog": {
        "Enrich": [
            "FromLogContext",
            "WithExceptionDetails"
        ],
        "Properties": {
            "Application": "HushifyApi"
        },
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.Hosting.Lifetime": "Information"
            }
        },
        "WriteTo": [
            {
                "Name": "Async",
                "Args": {
                    "configure": [
                        {
                            "Name": "Console",
                            "Args": {
                                "OutputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff} {Level:u3}] {Message:lj}{NewLine}{Exception}"
                            }
                        }
                    ]
                }
            }
        ]
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "DefaultConnection": ""
    },
    "Config": {
        "ApiUrl": {
            "Scheme": "https",
            "Domain": "api.hushify.io"
        },
        "WebUrls": [
            {
                "Scheme": "https",
                "Domain": "hushify.io"
            }
        ],
        "Rabbit": {
            "Host": "localhost",
            "VirtualHost": "/",
            "Username": "",
            "Password": ""
        },
        "Email": {
            "From": "noreply@hushify.io",
            "Host": "localhost",
            "Port": 1025,
            "Username": "",
            "Password": ""
        },
        "RSAKeyPair": {
            "PrivateKey": "",
            "PublicKey": ""
        },
        "CryptoBoxKeyPair": {
            "PrivateKey": "",
            "PublicKey": ""
        },
        "Jwt": {
            "Authority": "api.hushify.io",
            "ValidAudience": "hushify.io",
            "ValidIssuer": "api.hushify.io",
            "TokenValidityInMins": 30
        },
        "RefreshToken": {
            "TimeToLiveInDays": 7
        },
        "AWS": {
            "KeyId": null,
            "ServiceUrl": null,
            "BucketName": "",
            "AccessKey": "",
            "SecretKey": "",
            "Region": null,
            "PathStyle": false,
            "CloudFrontServiceUrl": null,
            "IsCloudFrontEnabled": false,
            "QueueName": null,
            "QueueRegion": null,
            "QueueAccessKey": null,
            "QueueSecretKey": null
        },
        "Stripe": {
            "PublishableKey": "",
            "SecretKey": "",
            "IsEnabled": false
        }
    }
}
