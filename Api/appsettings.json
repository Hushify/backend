{
    "Serilog": {
        "Enrich": [
            "FromLogContext",
            "WithExceptionDetails"
        ],
        "Properties": {
            "Application": "HushifyApi"
        },
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.Hosting.Lifetime": "Information"
            }
        },
        "WriteTo": [
            {
                "Name": "Async",
                "Args": {
                    "configure": [
                        {
                            "Name": "Console",
                            "Args": {
                                "OutputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} {Level} - {Message:lj}{NewLine}{Exception}"
                            }
                        }
                    ]
                }
            }
        ]
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "DefaultConnection": ""
    },
    "Config": {
        "RootDomain": "hushify.io",
        "ApiUrl": {
            "Scheme": "https",
            "Domain": "api.hushify.io"
        },
        "WebUrl": {
            "Scheme": "https",
            "Domain": "hushify.io"
        },
        "Rabbit": {
            "Host": "localhost",
            "VirtualHost": "/",
            "Username": "",
            "Password": ""
        },
        "Smtp": {
            "From": "noreply@hushify.io",
            "Host": "localhost",
            "Port": 1025,
            "Username": "",
            "Password": ""
        },
        "RSAKeyPair": {
            "PrivateKey": "",
            "PublicKey": ""
        },
        "CryptoBoxKeyPair": {
            "PrivateKey": "",
            "PublicKey": ""
        },
        "Jwt": {
            "Authority": "api.hushify.io",
            "ValidAudience": "hushify.io",
            "ValidIssuer": "api.hushify.io",
            "TokenValidityInMins": 30
        },
        "RefreshToken": {
            "TimeToLiveInDays": 7
        },
        "AWS": {
            "KeyId": "",
            "ServiceUrl": "",
            "BucketName": "",
            "AccessKey": "",
            "SecretKey": ""
        }
    },
    "FeatureManagement": {
        "FeatureFlags": {
            "Stripe": false
        }
    }
}
